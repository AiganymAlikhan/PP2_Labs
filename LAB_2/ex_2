# create lists
fruits = ["apple", "banana", "cherry"]
print(fruits)

print(fruits[0])  # apple

# add and delete elements
fruits.append("mango")
print(fruits)     #apple, banana, cherry, mango

fruits.remove("mango")
print(fruits)    #apple, banana, cherry


for fruit in fruits:
    print(fruit)
    '''
    apple
    banana
    cherry
    '''